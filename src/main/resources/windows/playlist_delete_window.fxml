<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="171.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane layoutX="3.0" prefHeight="52.0" prefWidth="600.0" style="-fx-background-color: #ededed;"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Label layoutX="53.0" layoutY="13.0" prefHeight="27.0" prefWidth="533.0"
               text="Delete Selected Playlist?" AnchorPane.leftAnchor="53.0" AnchorPane.rightAnchor="14.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <ImageView fitHeight="40.0" fitWidth="36.0" layoutX="14.0" layoutY="8.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../images/img_playlist_remove.png"/>
        </ImageView>
    </AnchorPane>
    <Label layoutX="14.0" layoutY="61.0" text="Are you sure you want to delete the playlist:"
           AnchorPane.leftAnchor="14.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label fx:id="labelPlaylist" layoutX="293.0" layoutY="61.0" prefHeight="20.0" prefWidth="226.0"
           textFill="#000dff" AnchorPane.leftAnchor="293.0" AnchorPane.rightAnchor="14.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="14.0" layoutY="81.0" text="This action is permanent and cannot be undone."
           AnchorPane.leftAnchor="14.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Button fx:id="btnDelete" layoutX="474.0" layoutY="365.0" mnemonicParsing="false" prefHeight="21.0"
            prefWidth="132.0" text="Delete" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <padding>
            <Insets bottom="2.0" top="2.0"/>
        </padding>
    </Button>
    <Button fx:id="btnCancel" layoutX="312.0" layoutY="365.0" mnemonicParsing="false" prefHeight="21.0"
            prefWidth="132.0" text="Cancel" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="156.0">
        <padding>
            <Insets bottom="2.0" top="2.0"/>
        </padding>
    </Button>
</AnchorPane>
