<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="538.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1">
    <Button fx:id="btnFinish" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" prefHeight="21.0"
            prefWidth="125.0" text="Apply" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <padding>
            <Insets bottom="2.0" top="2.0"/>
        </padding>
    </Button>
    <Button fx:id="btnCancel" layoutX="329.0" layoutY="365.0" mnemonicParsing="false" prefWidth="125.0"
            text="Cancel" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="146.0">
        <padding>
            <Insets bottom="2.0" top="2.0"/>
        </padding>
    </Button>
    <ListView fx:id="listViewEntries" layoutY="86.0" prefHeight="398.0" prefWidth="504.0"
              AnchorPane.bottomAnchor="54.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="86.0"/>
    <AnchorPane prefHeight="52.0" prefWidth="600.0" style="-fx-background-color: #ededed;"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Label layoutX="53.0" layoutY="13.0" prefHeight="27.0" prefWidth="533.0" text="Add Entries"
               AnchorPane.leftAnchor="53.0" AnchorPane.rightAnchor="14.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <ImageView fitHeight="40.0" fitWidth="36.0" layoutX="14.0" layoutY="8.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../images/img_playlist_modify.png"/>
        </ImageView>
    </AnchorPane>
    <Label layoutX="14.0" layoutY="60.0" text="Select the entries you wish to be stored in the playlist: "
           AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="60.0"/>
    <Label fx:id="labelPlaylist" layoutX="304.0" layoutY="60.0" prefHeight="17.0" prefWidth="131.0"
           textFill="#3300ff" AnchorPane.leftAnchor="304.0" AnchorPane.rightAnchor="14.0"
           AnchorPane.topAnchor="60.0"/>
</AnchorPane>
