<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[[%-5level] on [%t] at [%d{HH:mm:ss(.SSS)}] in [%logger{36}]]:%n\t\t %msg%n" />
        </Console>
        <File name="LatestLog" fileName="logs/I-Series (latest).log" immediateFlush="false" append="false">
            <PatternLayout pattern="[[%-5level] on [%t] at [%d{HH:mm:ss(.SSS)}] in [%logger{36}]]:%n\t\t %msg%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console" />
            <AppenderRef ref="LatestLog"/>
        </Root>
    </Loggers>
</Configuration>